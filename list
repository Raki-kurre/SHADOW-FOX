# Initial Justice League list
justice_league = ["Superman", "Batman", "Wonder Woman", "Flash", "Aquaman", "Green Lantern"]

# Step 1: Calculate the number of members in the Justice League
print("Step 1: Number of members in the Justice League:", len(justice_league))

# Step 2: Add Batgirl and Nightwing as new members
justice_league.extend(["Batgirl", "Nightwing"])
print("Step 2: After adding Batgirl and Nightwing:", justice_league)

# Step 3: Move Wonder Woman to the beginning of the list
justice_league.remove("Wonder Woman")
justice_league.insert(0, "Wonder Woman")
print("Step 3: After making Wonder Woman the leader:", justice_league)

# Step 4: Move Green Lantern between Aquaman and Flash
justice_league.remove("Green Lantern")
aquaman_index = justice_league.index("Aquaman")
justice_league.insert(aquaman_index + 1, "Green Lantern")
print("Step 4: After separating Aquaman and Flash:", justice_league)

# Step 5: Replace the existing list with a new team
justice_league = ["Cyborg", "Shazam", "Hawkgirl", "Martian Manhunter", "Green Arrow"]
print("Step 5: New Justice League team:", justice_league)

# Step 6: Sort the list alphabetically and identify the new leader
justice_league.sort()
print("Step 6: Justice League sorted alphabetically:", justice_league)
print("New leader of the Justice League:", justice_league[0])
